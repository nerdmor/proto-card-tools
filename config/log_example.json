{
    "streams": [
        {
            "type": "console",
            "level": "info",
            "format": "%(message)s"
        },
        {
            "type": "file",
            "level": "info",
            "path": "logs/log.txt",
            "format": "%(asctime)s | %(levelname)s | %(message)s",
            "max_bytes": 1000000
        }
    ]
}